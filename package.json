{
  "name": "trivo-ui-library",
  "version": "1.0.14",
  "type": "module",
  "description": "A modern React UI component library built with TypeScript",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": {
      "types": "./dist/styles.d.ts",
      "default": "./dist/trivo-ui-library.css"
    },
    "./variables.css": {
      "types": "./dist/styles.d.ts",
      "default": "./dist/variables.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && node -e \"require('fs').copyFileSync('src/styles/variables.css', 'dist/variables.css'); require('fs').copyFileSync('src/styles.d.ts', 'dist/styles.d.ts')\"",
    "dev": "vite build --watch",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "typescript",
    "ui-library",
    "components",
    "trivo"
  ],
  "author": "Tri Vo Emerald",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trivoemerald/trivo-ui-library.git"
  },
  "bugs": {
    "url": "https://github.com/trivoemerald/trivo-ui-library/issues"
  },
  "homepage": "https://github.com/trivoemerald/trivo-ui-library#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@radix-ui/react-slot": "^1.2.4",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@storybook/addon-a11y": "^10.0.2",
    "@storybook/addon-docs": "^10.0.2",
    "@storybook/addon-vitest": "^10.0.2",
    "@storybook/react-vite": "^10.0.2",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/browser-playwright": "^4.0.6",
    "@vitest/coverage-v8": "^4.0.6",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cssnano": "^7.1.2",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rollup": "^4.52.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^10.0.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@types/react-datepicker": "^6.2.0",
    "lucide-react": "^0.553.0",
    "react-datepicker": "^8.9.0",
    "vite-plugin-dts": "^4.5.4"
  }
}
